		"solid-meta": "^0.28.1"
	},
	"devDependencies": {
		"@solidjs/router": "^0.4.2",
		"cross-env": "^7.0.3",
		"vite": "^3.0.8",
		"vite-plugin-solid": "^2.3.0"
	}
}
